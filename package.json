{
  "name": "AlbuminDietMobapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "npx jetify",
    "tsc": "tsc",
    "update-types": "yarn add --dev albumin-diet-types@latest",
    "release": "sh bundle-android.sh"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-paper": "^2.15.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "^11.2.4",
    "react-navigation": "^3.5.1",
    "react-navigation-material-bottom-tabs": "^1.0.0",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "urlfactory-js": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.8.4",
    "@types/react-native": "^0.63.51",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-navigation-material-bottom-tabs": "^0.3.1",
    "@types/react-redux": "^7.0.9",
    "@types/react-test-renderer": "^16.8.3",
    "albumin-diet-types": "^0.0.19",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.59.0",
    "path": "^0.12.7",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "16.13.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
